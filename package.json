    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "razorpay": "^2.9.6",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
